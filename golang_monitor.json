{
  "steps": [
    {
      "id": "step1",
      "name": "Identify GitHub API endpoint for Go releases",
      "type": "tool",
      "tool": "fetch",
      "inputs": {
        "url": "https://api.github.com/repos/golang/go/releases/latest"
      },
      "outputs": {
        "latest_release": "release_data"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    },
    {
      "id": "step2",
      "name": "Parse and extract release information",
      "type": "tool",
      "tool": "memory",
      "inputs": {
        "key": "release_data",
        "value": "Extract tag, published_at, and body from the response"
      },
      "outputs": {
        "parsed_release": "release_info"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    },
    {
      "id": "step3",
      "name": "Analyze release notes for new features or breaking changes",
      "type": "tool",
      "tool": "sequential-thinking",
      "inputs": {
        "context": "The release is for the golang/go repository. Focus on significant changes relevant to developers.",
        "prompt": "Analyze the following GitHub release notes for new features, breaking changes, or important updates. Summarize in 3 bullet points. Include whether this is a major or minor release.\nRelease notes: {{release_info}}"
      },
      "outputs": {
        "analysis_summary": "summary"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    },
    {
      "id": "step4",
      "name": "Present the latest Go release and analysis",
      "type": "tool",
      "tool": "memory",
      "inputs": {
        "key": "summary",
        "value": "Format as a readable summary for developers: Latest Go release: {{release_info.tag}} ({{release_info.published_at}}). Summary: {{summary}}"
      },
      "outputs": {
        "final_output": "output"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    }
  ],
  "triggers": null,
  "variables": {},
  "error_policy": {
    "strategy": "",
    "max_retries": 0,
    "retry_delay": 0
  },
  "timeout": 0,
  "metadata": {}
}