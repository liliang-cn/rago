{
  "steps": [
    {
      "id": "step1",
      "name": "Fetch GitHub repository updates",
      "type": "tool",
      "tool": "fetch",
      "inputs": {
        "url": "https://github.com/liliang-cn/rago"
      },
      "outputs": {
        "github_response": "github_data"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    },
    {
      "id": "step2",
      "name": "Analyze GitHub updates for new features or releases",
      "type": "tool",
      "tool": "sequential-thinking",
      "inputs": {
        "prompt": "Analyze the GitHub repository page content to identify recent changes, new features, or releases. Summarize the key updates in bullet points."
      },
      "outputs": {
        "analysis_summary": "github_analysis"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    },
    {
      "id": "step3",
      "name": "Store the results for further use",
      "type": "tool",
      "tool": "memory",
      "inputs": {
        "key": "rago_new_updates",
        "value": "{{github_analysis}}"
      },
      "outputs": {
        "stored_result": "stored_output"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    }
  ],
  "triggers": null,
  "variables": {},
  "error_policy": {
    "strategy": "",
    "max_retries": 0,
    "retry_delay": 0
  },
  "timeout": 0,
  "metadata": {}
}