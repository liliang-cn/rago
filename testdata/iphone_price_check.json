{
  "steps": [
    {
      "id": "step1",
      "name": "Fetch Latest iPhone Price Data",
      "type": "tool",
      "tool": "fetch",
      "inputs": {
        "url": "https://www.apple.com/iphone/prices/"
      },
      "outputs": {
        "iphone_prices": "latest_prices"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    },
    {
      "id": "step2",
      "name": "Fetch Recent iPhone News and Releases",
      "type": "tool",
      "tool": "fetch",
      "inputs": {
        "url": "https://newsapi.org/v2/everything?q=iphone+release\u0026apiKey=your_news_api_key"
      },
      "outputs": {
        "news_updates": "recent_news"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    },
    {
      "id": "step3",
      "name": "Analyze iPhone Value and Worth of Purchase",
      "type": "tool",
      "tool": "sequential-thinking",
      "inputs": {
        "prompt": "Based on the latest iPhone prices and recent news, analyze whether it's worth buying now. Consider factors like price trends, new features, and market availability. Provide a clear recommendation with reasoning."
      },
      "outputs": {
        "analysis_result": "worth_buying"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    },
    {
      "id": "step4",
      "name": "Store Final Recommendation",
      "type": "tool",
      "tool": "memory",
      "inputs": {
        "key": "iphone_purchase_recommendation",
        "value": "{{worth_buying}}"
      },
      "outputs": {
        "stored_recommendation": "recommendation"
      },
      "retry": {
        "enabled": false,
        "max_retries": 0,
        "delay": 0,
        "backoff_type": "",
        "max_delay": 0
      },
      "timeout": 0
    }
  ],
  "triggers": null,
  "variables": {},
  "error_policy": {
    "strategy": "",
    "max_retries": 0,
    "retry_delay": 0
  },
  "timeout": 0,
  "metadata": {}
}